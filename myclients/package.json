{
  "name": "myclients",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start-web": "run-p lib-shared:build lib-shared-ui:waiton web:start-waiton",
    "lib-shared:build": "ng build lib-shared --watch",
    "lib-shared-ui:waiton": "wait-on dist/lib-shared/package.json && npm run lib-shared-ui:build",
    "lib-shared-ui:build": "ng build lib-shared-ui --watch",
    "web:start-waiton": "wait-on dist/lib-shared-ui/package.json && wait-on dist/lib-shared/package.json && npm run web:start",
    "web:start": "ng serve app-web"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "font-awesome": "4.7.0",
    "npm-run-all": "^4.1.5",
    "rxjs": "~7.5.0",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "tslib": "^2.3.0",
    "wait-on": "^7.0.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.0.0",
    "typescript": "~4.8.2"
  }
}
